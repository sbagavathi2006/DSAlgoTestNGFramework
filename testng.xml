<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="DSAlgo_TestNG_Framework Test Suite" parallel="tests" thread-count="2">
	<listeners>
		<listener class-name="listeners.ExtentReportTestNGListener"/> 
		<listener class-name="com.aventstack.chaintest.plugins.ChainTestListener"/>
    	<listener class-name="listeners.AnnotationTransformer"/> 
	</listeners>
  
 <test name="chrome">
    <parameter name="browser" value="chrome"/>
    <classes>
      <class name="testclasses.DSPortalTest"/>
      <class name="testclasses.HomeTest"/>
      <class name="testclasses.LoginTest"/>
      <class name="testclasses.ArrayTest"/>
      <class name="testclasses.GraphTest"/>
      <class name="testclasses.QueueTest"/>
    </classes>
  </test> 

  <test name="edge">
    <parameter name="browser" value="edge"/>
    <classes>
      <class name="testclasses.DSPortalTest"/>
      <class name="testclasses.HomeTest"/>
      <class name="testclasses.LoginTest"/>
      <class name="testclasses.ArrayTest"/>
      <class name="testclasses.GraphTest"/>
      <class name="testclasses.QueueTest"/>
    </classes>
  </test>
  
  
</suite>